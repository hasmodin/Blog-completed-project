<% layout("layouts/boilerplate.ejs") -%>

    <% for(blog of blogs) { %>
  <a href="http://localhost:3030/blogs/<%=blog._id %>">
    <div class="blog-card">
        <div class="img">
            <img src="<%= blog.image.url %>" alt="Blog image">
          </div>
          <div class="title">
            <h1><%= blog.title %></h1>
            <p><%= blog.summary %></p>
          </div>
      </div>
  </a>
<% } %>
<br><br><br><br><br>

